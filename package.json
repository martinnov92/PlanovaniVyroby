  "private": true,
  "homepage": "./",
  "author": "Martin NovÃ¡k",
  "main": "public/electron.js",
  "repository": "https://github.com/martinnov92/PlanovaniVyroby",
  "dependencies": {
    "chokidar": "^2.0.4",
    "electron-updater": "^3.1.6",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "moment-range": "^4.0.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-scripts": "2.1.1",
    "react-tabs": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "electron": "^3.0.7",
    "electron-builder": "^20.29.0",
    "wait-on": "^3.1.0"
  },
  "scripts": {
    "electron": "electron .",
    "start": "react-scripts start",
    "eject": "react-scripts eject",
    "build-react": "react-scripts build",
    "build-windows": "electron-builder -w",
    "test": "react-scripts test --env=jsdom",
    "build-mac": "react-scripts build && electron-builder -m",
    "ship-mac": "react-scripts build && electron-builder -m -p always",
    "ship-windows": "react-scripts build && electron-builder -w -p always",
    "dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "build": {
    "appId": "com.martinnovak.PlanovaniVyroby",
    "productName": "Planovani Vyroby",
    "icon": "build/icon.png",
    "copyright": "Copyright 2018 year ${author}",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "martinnov92",
        "repo": "PlanovaniVyroby"
      }
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
